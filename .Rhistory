devtools::install_github('DianaYuanZhou/ISTA-Package')
library(ISTA)
data("bikeshare")
head(bikeshare)
n <- nrow(bikeshare)
data.X <- as.matrix(bikeshare[sample(1:n, 700),3:12])
data.Y <- as.matrix(bikeshare[sample(1:n, 700),'cnt'])
MSE.result <- ISTA.main(data.X, data.Y, method = c('CV', 'AIC', 'BIC'), lambda = seq(1e-4, 1e-1, len = 100))
?ISTA
library(ISTA)
library(ISTA)
source(Main_func.R)
ISTA.main(data.X, data.Y)
